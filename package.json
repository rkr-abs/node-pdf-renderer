{
  "name": "",
  "version": "0.0.0",
  "description": "",

  "main": "./index.js",
  "scripts": {
    "start": "npm run build && npm run dev",
    "dev": "node dist/index.js",
    "test": "jest --no-cache",
    "build": "rm -rf ./dist && babel --ignore '**/*.spec.js' -d ./dist ./src && npm run copy",
    "prepareDist": "npm run lint && npm test && npm run build",
    "dist": "npm run prepareDist && cd dist && npm publish --access public && cd ..",
    "copy": "cp ./package.json ./ReadMe.md ./LICENSE ./dist/",
    "lint": "eslint ."
  },
  "engines": {
    "node": ">=7.6"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "author": "Laufire Technologies",
  "license": "ISC",
  "homepage": "",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.7.2",
    "acorn": "^6.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "eslint": "^5.16.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "pino-pretty": "^3.2.2",
    "regenerator-runtime": "^0.13.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**/*.js"
    ],
    "coverageDirectory": "./.coverage",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "resetMocks": true,
    "resetModules": true,
    "setupFilesAfterEnv": [
      "./test/setup/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "dependencies": {
    "@react-pdf/renderer": "^3.1.9",
    "pino": "^5.13.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
